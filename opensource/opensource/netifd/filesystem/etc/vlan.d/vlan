config switch
        option name 'switch0'
        option enable_vlan '1'
        option reset '1'

config switch_vlan
        option ports '0t 1 2 3 4'
        option device 'switch0'
        option vlan '1'
        option vid '1'

config switch_vlan
        option ports '0t 5'
        option device 'switch0'
        option vlan '2'
        option vid '2'
        
